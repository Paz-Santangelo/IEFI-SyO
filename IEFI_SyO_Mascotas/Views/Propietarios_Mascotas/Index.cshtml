@model IEnumerable<IEFI_SyO_Mascotas.Models.Propietarios_Mascotas>

@{
    ViewBag.Title = "Lista Propietarios";
}

<h2>Lista de Propietarios</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nombre_Propietario)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Telefono)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.email)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nombre_Propietario)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Telefono)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.email)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id_Propietario }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id_Propietario }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id_Propietario })
        </td>
    </tr>
}

</table>
